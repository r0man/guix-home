{
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "includeCoAuthoredBy": false,
  "mcpServers": {
    "clojure-mcp": {
      "type": "stdio",
      "command": "clojure-mcp",
      "args": [],
      "env": {}
    }
  },
  "model": "opus",
  "permissions": {
    "allow": [
      "Bash(guix:*)"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/**)"
    ]
  }
}
